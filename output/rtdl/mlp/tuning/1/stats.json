{
    "config": {
        "base_config": {
            "data": {
                "normalization": "quantile",
                "path": "data/rtdl",
                "y_policy": "mean_std"
            },
            "model": {},
            "seed": 1,
            "training": {
                "batch_size": 1024,
                "eval_batch_size": 8192,
                "n_epochs": 1000000000,
                "optimizer": "radam",
                "patience": 16
            }
        },
        "optimization": {
            "options": {
                "n_trials": 1000
            },
            "sampler": {
                "seed": 1
            },
            "space": {
                "model": {
                    "d_layers": [
                        "$mlp_d_layers",
                        1,
                        8,
                        1,
                        1024
                    ],
                    "dropout": [
                        "?uniform",
                        0.0,
                        0.0,
                        0.5
                    ]
                },
                "training": {
                    "lr": [
                        "loguniform",
                        1e-05,
                        0.01
                    ],
                    "weight_decay": [
                        "?loguniform",
                        0.0,
                        1e-06,
                        0.001
                    ]
                }
            }
        },
        "program": "bin/mlp.py"
    },
    "environment": {
        "devices": {
            "CUDA_VISIBLE_DEVICES": "0",
            "torch.version.cuda": "10.2",
            "torch.backends.cudnn.version()": 7605,
            "driver": "516.59",
            "0": {
                "name": "NVIDIA GeForce RTX 2070",
                "total_memory": 8589934592
            }
        }
    }
}
